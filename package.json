{
  "name": "zip-to-msa-api",
  "version": "1.0.0",
  "description": "API that accepts ZIP codes and outputs MSA and Population",
  "main": "server/index.ts",
  "scripts": {
    "dev": "nodemon",
    "docker-dev": "docker-compose -f docker-compose.dev.yml up",
    "docker-prod": "docker-compose up",
    "docker-test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker-update": "bash ./scripts/docker-update.sh",
    "start": "NODE_ENV=production ts-node src/server",
    "test": "NODE_ENV=test jest",
    "test-coverage": "NODE_ENV=test jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vartanovs/zip-to-msa-api.git"
  },
  "author": "Serge Vartanov <vartanov.s@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vartanovs/zip-to-msa-api/issues"
  },
  "homepage": "https://github.com/vartanovs/zip-to-msa-api#readme",
  "dependencies": {
    "csv-parse": "4.4.6",
    "csv-stringify": "5.3.0",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "node-fetch": "2.6.0",
    "ts-node": "8.2.0",
    "typescript": "3.5.1"
  },
  "devDependencies": {
    "@types/csv-stringify": "^1.4.2",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/fetch-mock": "^7.2.0",
    "@types/jest": "24.0.13",
    "@types/node-fetch": "^2.1.4",
    "fetch-mock": "7.3.3",
    "jest": "24.8.0",
    "node-mocks-http": "1.7.5",
    "nodemon": "^1.19.1",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0"
  }
}
